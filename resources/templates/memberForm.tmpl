		{{/* <link th:href="@{/css/wisvch-dashboard.css}" rel="stylesheet">*/}}
		{{/* <link rel="stylesheet" th:href="@{/webjars/font-awesome/5.0.13/web-fonts-with-css/css/fontawesome-all.min.css}">*/}}
	</head>

	<body>
		<form action="/api/form" method="POST" class="form col-12">
			<p>Simple user form</p>
			<div class="form-group ">
				<label for="first_name">first name</label>
				<input type="text" name="first_name" class="form-control" id="first_name" value="">
			</div>
			<div class="form-group ">
				<label for="last_name">last name</label>
				<input type="text" name="last_name" class="form-control" id="last_name" value="">
			</div>
			<div class="form-group ">
				<label for="gender">gender</label>
				<select name="gender" id="gender" class="form-control">
					<option value="male">Male</option>
					<option value="female">Female</option>
					<option value="other">Other</option>
				</select>
			</div>
			<div class="form-group ">
				<label for="email">email</label>
				<input type="text" name="email" class="form-control" id="email" value="">
			</div>
			<div class="form-group ">
				<label for="phone_number">phone number</label>
				<input type="text" name="phone_number" class="form-control" id="phone_number" value="">
			</div>
			<div class="form-group ">
				<label for="full_name">full_name</label>
				<input type="text" name="full_name" class="form-control" id="full_name" value="">
			</div>
			<div class="form-group ">
				<label for="street_name">street_name</label>
				<input type="text" name="street_name" class="form-control" id="street_name" value="">
			</div>
			<div class="form-group ">
				<label for="street_number">street_number</label>
				<input type="text" name="street_number" class="form-control" id="street_number" value="">
			</div>
			<div class="form-group ">
				<label for="postal_code">postal_code</label>
				<input type="text" name="postal_code" class="form-control" id="postal_code" value="">
			</div>
			<div class="form-group ">
				<label for="place">place</label>
				<input type="text" name="place" class="form-control" id="place" value="">
			</div>
			<div class="form-group ">
				<label for="country">country</label>
				<input type="text" name="country" class="form-control" id="country" value="">
			</div>
			<div class="form-group ">
				<label for="first_letters">first_letters</label>
				<input type="text" name="first_letters" class="form-control" id="first_letters" value="">
			</div>
			<div class="form-group ">
				<label for="netid">netid</label>
				<input type="text" name="netid" class="form-control" id="netid" value="">
			</div>
			<div class="form-check ">
				<label for="yearbook_permission">yearbook_permission</label>
				<input type="checkbox" name="yearbook_permission" class="form-check-input" id="yearbook_permission"
					   value="">
			</div>
			<div class="form-check ">
				<label for="activity_mail">activity_mail</label>
				<input type="checkbox" name="activity_mail" class="form-check-input" id="activity_mail" value="">
			</div>
			<div class="form-check ">
				<label for="career_mail">career_mail</label>
				<input type="checkbox" name="career_mail" class="form-check-input" id="career_mail" value="">
			</div>
			<div class="form-check ">
				<label for="education_mail">education_mail</label>
				<input type="checkbox" name="education_mail" class="form-check-input" id="education_mail" value="">
			</div>
			<div class="form-check ">
				<label for="machazine">machazine</label>
				<input type="checkbox" name="machazine" class="form-check-input" id="machazine" value="">
			</div>
			<div class="form-check ">
				<label for="freshmen_weekend">freshmen_weekend</label>
				<input type="checkbox" name="freshmen_weekend" class="form-check-input" id="freshmen_weekend" value="">
			</div>
			<input type="submit">
		</form>
	</body>

</html>
